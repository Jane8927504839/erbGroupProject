class=\"section1\">\n          <div class=\"img-slider\">\n            <img src=\"https://images.pexels.com/photos/6347888/pexels-photo-6347888.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260\" alt=\"\" class=\"img\">\n            <img src=\"https://images.pexels.com/photos/3962294/pexels-photo-3962294.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260\" alt=\"\" class=\"img\">\n            <img src=\"https://images.pexels.com/photos/2292953/pexels-photo-2292953.jpeg?auto=compress&cs=tinysrgb&dpr=2&w=500\" alt=\"\" class=\"img\">\n            <img src=\"https://images.pexels.com/photos/1229861/pexels-photo-1229861.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260\" alt=\"\" class=\"img\">\n            <img src=\"https://images.pexels.com/photos/1598505/pexels-photo-1598505.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260\" alt=\"\" class=\"img\">\n          </div>\n        </div>\n        <div class=\"section2\">\n          <div class=\"container\"></div>\n        </div>\n      </div>\n    </section>\n    <div class=\"home\">\n      <!-- 轮播图 -->\n      <div id=\"carouselExampleIndicators\" class=\"carousel slide\" data-bs-ride=\"carousel\">\n        <div class=\"carousel-inner\">\n          <div class=\"carousel-item active\">\n            <img src=\"../assets/banner1.jpg\" class=\"d-block w-100\" alt=\"厨具展示\">\n          </div>\n          <!-- 更多轮播项... -->\n        </div>\n      </div>\n\n      <!-- 产品分类 -->\n      <div class=\"container mt-5\">\n        <h2 class=\"text-center mb-4\">产品分类</h2>\n        <div class=\"row\">\n          <div class=\"col-md-4 mb-4\" v-for=\"category in categories\" :key=\"category.name\">\n            <div class=\"card category-card\" @click=\"goToCategory(category.name)\">\n              <img :src=\"category.image\" class=\"card-img-top\" :alt=\"category.name\">\n              <div class=\"card-body\">\n                <h5 class=\"card-title\">{{ category.name }}</h5>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 热门产品 -->\n      <div class=\"container mt-5\">\n        <h2 class=\"text-center mb-4\">热门产品</h2>\n        <div class=\"row\">\n          <div class=\"col-md-3 mb-4\" v-for=\"product in hotProducts\" :key=\"product._id\">\n            <div class=\"card product-card\">\n              <img :src=\"product.images[0]\" class=\"card-img-top\" :alt=\"product.name\">\n              <div class=\"card-body\">\n                <h5 class=\"card-title\">{{ product.name }}</h5>\n                <p class=\"card-text text-danger\">¥{{ product.price }}</p>\n                <button @click=\"addToCart(product)\" class=\"btn btn-primary\">\n                  加入购物车\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <footer>\n      <div class=\"footer0\">\n        <h1>ShoPperZ</h1>\n      </div>\n      <div class=\"footer1 \">\n        Connect with us at\n        <div class=\"social-media\">\n          <a href=\"#\"><ion-icon name=\"logo-facebook\"></ion-icon></a>\n          <a href=\"#\"><ion-icon name=\"logo-linkedin\"></ion-icon></a>\n          <a href=\"#\"><ion-icon name=\"logo-youtube\"></ion-icon></a>\n          <a href=\"#\"><ion-icon name=\"logo-instagram\"></ion-icon></a>\n          <a href=\"#\"><ion-icon name=\"logo-twitter\"></ion-icon></a>\n        </div>\n      </div>\n      <div class=\"footer2\">\n        <div class=\"product\">\n          <div class=\"heading\">Products</div>\n          <div class=\"div\">Sell your Products</div>\n          <div class=\"div\">Advertise</div>\n          <div class=\"div\">Pricing</div>\n          <div class=\"div\">Product Buisness</div>\n        </div>\n        <div class=\"services\">\n          <div class=\"heading\">Services</div>\n          <div class=\"div\">Return</div>\n          <div class=\"div\">Cash Back</div>\n          <div class=\"div\">Affiliate Marketing</div>\n          <div class=\"div\">Others</div>